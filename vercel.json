{
  "builds": [
    {
      "src": "miniDice/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.13"
      }
    },
    {
      "src": "telegram_bot_run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.13"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "miniDice/wsgi.py"
    }
  ]
}